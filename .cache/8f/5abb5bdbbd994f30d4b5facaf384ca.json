der-radius: 50%;\r\n    width: 45px;\r\n    height: 45px;\r\n    margin-bottom: 0.8rem;\r\n    display: flex;\r\n    justify-content:center ;\r\n    align-items: center;;\r\n\r\n    img{\r\n        width: 40%;\r\n        height: 40%;\r\n    }\r\n}\r\n\r\n.header__title{\r\n    color: white;\r\n    font-size: 1.6rem;\r\n    margin-bottom: 0.8rem;\r\n}\r\n\r\n.rating{\r\n    display: flex;\r\n    justify-content: space-between;\r\n    align-items: center;\r\n    gap: auto;\r\n    margin-top: 1.5rem;\r\n\r\n    .number{\r\n        width: 45px;\r\n        height: 45px;\r\n        border-radius: 50%;\r\n        background-color: $dark-Blue;\r\n        color: $medium-grey;\r\n        font-size: 0.7rem;\r\n        font-weight: 400;\r\n        display: flex;\r\n        justify-content: center;\r\n        align-items: center;\r\n        padding: 0;\r\n        margin: 0;\r\n        border: none;\r\n        cursor: pointer;\r\n\r\n        &:hover{\r\n            background-color: $orange;\r\n            color: white;\r\n        }\r\n    }\r\n\r\n    .active{\r\n        background-color: $light-grey;\r\n        color: white;\r\n    }\r\n}\r\n\r\n.submit-button{\r\n    font-family: \"Overpass\", sans-serif;\r\n    height: 50px;\r\n    width: 100%;\r\n    border-radius: 50px;\r\n    background-color: $orange;\r\n    color: white;\r\n    font-weight: 400;\r\n    font-size: 1rem;\r\n    letter-spacing: 0.3ch;\r\n    margin-top: 1.5rem;\r\n    border: 0;\r\n    cursor: pointer;\r\n\r\n    &:hover{\r\n        background-color: white;\r\n        color: $orange;\r\n    }\r\n}\r\n\r\n.hide{\r\n    display: none;\r\n}\r\n\r\n.card-2 {\r\n    background: linear-gradient(15deg, $very-dark-Blue 0%, $dark-Blue 100%);\r\n    padding: 25px;\r\n    border-radius: 10px;\r\n    width: 350px;\r\n    height: 370px;\r\n}\r\n\r\n.card__header{\r\n    width: 100%;\r\n    height: 50%;\r\n    display: flex;\r\n    flex-direction: column;\r\n    justify-content: space-between;\r\n    align-items: center;\r\n}\r\n\r\n.title__image{\r\n    width: 100%;\r\n    height: 60%;\r\n    object-fit: contain;\r\n    margin-top: 0.5rem;\r\n    display: flex;\r\n    justify-content: center;\r\n    align-items: center;\r\n\r\n    img{\r\n        height: 100%;\r\n    }\r\n}\r\n\r\n.rating-container{\r\n    height: 2rem;\r\n    background-color: $dark-Blue;\r\n    border-radius: 50px;\r\n    margin: 0 auto;\r\n    display: flex;\r\n    justify-content: center;\r\n    align-items: center;\r\n\r\n    h3{\r\n        padding: 1em;\r\n        color: $orange;\r\n        font-size: 1rem;\r\n        font-weight: 400;\r\n    }\r\n}\r\n\r\n.card__body{\r\n    display: flex;\r\n    flex-direction: column;\r\n    justify-content: space-between;\r\n    align-items: center;\r\n\r\n    h1{\r\n        margin-top: 1.5rem;\r\n        margin-bottom: 1rem;\r\n        color: white;\r\n        font-size: 1.6rem;\r\n        font-weight: 700;\r\n    }\r\n\r\n    p{\r\n        text-align: center;\r\n    }\r\n}\r\n\r\n"},"lineCount":null}},"error":null,"hash":"1cb40760e3ae8a84c813dce67e0f1251","cacheData":{"env":{}}}